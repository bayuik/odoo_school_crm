<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="sekolah_mata_pelajaran_tree" model="ir.ui.view">
        <field name="name">sekolah.mata.pelajaran.tree</field>
        <field name="model">sekolah.mata.pelajaran</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="jurusan"/>
            </tree>
        </field>
    </record>

    <record id="sekolah_mata_pelajaran_form" model="ir.ui.view">
        <field name="name">sekolah.mata.pelajaran.form</field>
        <field name="model">sekolah.mata.pelajaran</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                        <group>
                            <field name="jurusan"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_sekolah_mata_pelajaran" model="ir.actions.act_window">
        <field name="name">Mata Pelajaran</field>
        <field name="res_model">sekolah.mata.pelajaran</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>